  <body ng-app="resumeApp" class="home header-has-img" ng-controller="wrapCtrl">

  <div class="head-bg" ng-style="{'background-image': 'url(' + headerbg + ')'}">></div>

  <div class="head-bar animated" ng-controller="NavCtrl">
                <div class="head-bar-inner">
                    <div class="row">
                        <div class="col-sm-3 col-xs-6 col-sm-push-1">
                            <div class="nav-wrap left">
                                <nav id="nav" class="nav">
                                    <img class="am-logo light" ng-src="{{logoLight}}"></img>
                                    <a scroll-to="nav"><img class="am-logo dark" ng-src="{{logoDark}}"></img></a>
                                </nav>
                            </div>
                        </div>
        
                        <div class="col-sm-6 col-xs-6">
                            <div class="nav-wrap">
                                <nav id="nav" class="nav col-sm-12 text-center main-menu">
                                    <ul class="clearfix">
                                        <li><a ng-click="goHome()" target="_self" scroll-to="aboutsection" offset="120">About<span></span></a></li>
                                        <li><a ng-click="goHome()" scroll-to="references" offset="120" target="_self">References<span></span></a></li>
                                        <li><a ng-click="goHome()" scroll-to="skills" offset="120" callback-before="goHome()" target="_self">Technologies<span></span></a></li>
                                        <li><a ng-click="goHome()" scroll-to="abilities" offset="120" target="_self">Skills<span></span></a></li>
                                        <li><a ng-click="goHome()" scroll-to="projects" offset="120" target="_self">Projects<span></span></a></li>
                                    </ul>
                                </nav>
                            </div>
                            <!-- .nav-wrap -->
                        </div>
                        <div class="col-sm-3 col-xs-6 col-sm-pull-1 login-menu">
                            <div class="nav-wrap">
                                <nav id="nav" class="nav">
                                    <ul class="clearfix" ng-hide="isLoggedIn()">
                                        <li><a href="#login">Login<span></span></a></li>
                                        <li><a href="/#register">Register<span></span></a></li>
                                    </ul>
                                    <ul class="clearfix" ng-show="isLoggedIn()">
                                        <li><a ng-click="goDash()" scroll-to="nav" offset="120">Dashboard</a><span></span></li>
                                        <li><a href="#" ng-click="logOut()">Logout</a><span></span></li>
                                    </ul>
                                </nav>
                            </div>
                        </div>

                    </div>
                </div>
            </div>

        <div class="container" id="content">

      <ui-view></ui-view>
    </div>
  </body>
</html>